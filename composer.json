{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "laravel/framework": "^11.9",
        "laravel/horizon": "^5.29",
        "laravel/pulse": "^1.2",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.9",
        "opcodesio/log-viewer": "^3.11",
        "spatie/laravel-data": "^4.10",
        "spatie/laravel-route-attributes": "^1.23",
        "spatie/laravel-settings": "^3.4",
        "symfony/error-handler": "^7.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.3",
        "pestphp/pest-plugin-laravel": "^3.0",
        "qossmic/deptrac": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/",
            "Presentation\\": "src/Presentation/",
            "Shared\\": "src/Shared/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "optimize:clear": [
            "@php artisan optimize:clear",
            "@php artisan filament:optimize-clear"
        ],
        "optimize": [
            "@php artisan optimize",
            "@php artisan filament:optimize",
            "@php artisan data:cache-structures --show-classes"
        ],
        "pint": [
            "./vendor/bin/pint --dirty --test"
        ],
        "pint:fix": [
            "./vendor/bin/pint --dirty"
        ],
        "pint:all": [
            "./vendor/bin/pint --test"
        ],
        "pint:fix-all": [
            "./vendor/bin/pint"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "pre-commit": [
            "composer pint:fix-all",
            "composer phpstan"
        ],
        "ide-helper:model": [
            "@php artisan ide-helper:models --write -R"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
